<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="clr-namespace:WalletWasabi.Fluent.Controls"
             xmlns:addWallet="clr-namespace:WalletWasabi.Fluent.ViewModels.AddWallet"
             xmlns:i="using:Avalonia.Xaml.Interactivity"
             xmlns:behaviors="using:WalletWasabi.Fluent.Behaviors"
             mc:Ignorable="d" d:DesignWidth="800"
             x:Class="WalletWasabi.Fluent.Views.AddWallet.WelcomePageView"
             x:DataType="addWallet:WelcomePageViewModel"
             x:CompileBindings="True">
  <i:Interaction.Behaviors>
    <behaviors:FocusOnAttachedBehavior />
  </i:Interaction.Behaviors>
  <controls:ContentArea EnableCancel="False"
                        EnableBack="False"
                        EnableNext="True"
                        ClipToBounds="False"
                        NextContent="{Binding NextLabel}"
                        ScrollViewer.VerticalScrollBarVisibility="Disabled">

    <DockPanel LastChildFill="True">

      <!-- Indicator & Buttons -->
      <Panel DockPanel.Dock="Bottom">

        <ListBox HorizontalAlignment="Center"
                 SelectedIndex="{Binding SelectedIndex}"
                 Background="Transparent"
                 IsHitTestVisible="False">
          <ListBox.Styles>
            <Style Selector="ListBoxItem:selected /template/ ContentPresenter#PART_ContentPresenter">
              <Setter Property="Background" Value="Transparent" />
            </Style>
            <Style Selector="ListBoxItem:selected /template/ ContentPresenter">
              <Setter Property="TextBlock.Foreground" Value="{DynamicResource SystemChromeAltLowColor}" />
            </Style>
            <Style Selector="ListBoxItem">
              <Setter Property="Opacity" Value="0.3" />
            </Style>
            <Style Selector="ListBoxItem:selected">
              <Setter Property="Opacity" Value="1" />
            </Style>
            <Style Selector="Ellipse">
              <Setter Property="Fill" Value="{DynamicResource SystemChromeAltLowColor}" />
              <Setter Property="Width" Value="9" />
              <Setter Property="Height" Value="9" />
            </Style>
          </ListBox.Styles>
          <ItemsControl.ItemsPanel>
            <ItemsPanelTemplate>
              <StackPanel Orientation="Horizontal" Spacing="5" />
            </ItemsPanelTemplate>
          </ItemsControl.ItemsPanel>
          <Ellipse />
          <Ellipse />
          <Ellipse />
          <Ellipse />
          <Ellipse />
        </ListBox>

        <!-- Carousel nav buttons -->
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="5">
          <StackPanel.Styles>
            <Style Selector="Button">
              <Setter Property="Width" Value="30" />
              <Setter Property="Foreground" Value="{DynamicResource SystemChromeAltLowColor}" />
              <Setter Property="Background" Value="{DynamicResource WelcomeScreenButtonBackground}" />
              <Setter Property="Height" Value="30" />
              <Setter Property="HorizontalContentAlignment" Value="Center" />
              <Setter Property="VerticalContentAlignment" Value="Center" />
              <Setter Property="Padding" Value="0" />
              <Setter Property="BorderThickness" Value="0" />
            </Style>
            <Style Selector="Button:disabled">
              <Setter Property="Opacity" Value="0.2" />
            </Style>
            <Style Selector="Button:pointerover /template/ ContentPresenter#PART_ContentPresenter">
              <Setter Property="Background" Value="{DynamicResource WelcomeScreenButtonBackgroundPointerOver}" />
              <Setter Property="TextBlock.Foreground" Value="{DynamicResource SystemChromeAltLowColor}" />
            </Style>
            <Style Selector="Button:pressed  /template/ ContentPresenter#PART_ContentPresenter">
              <Setter Property="Background" Value="{DynamicResource WelcomeScreenButtonBackgroundPressed}" />
              <Setter Property="TextBlock.Foreground" Value="{DynamicResource SystemChromeAltLowColor}" />
            </Style>
            <Style Selector="Button > PathIcon">
              <Setter Property="Width" Value="5" />
            </Style>
          </StackPanel.Styles>
        </StackPanel>
      </Panel>

      <!-- Pages -->
      <Carousel SelectedIndex="{Binding SelectedIndex}"
                DockPanel.Dock="Top"
                Margin="0 10">
        <i:Interaction.Behaviors>
          <behaviors:ExecuteCommandOnKeyDownBehavior Command="{Binding NextCommand}"
                                                     IsEnabled="{Binding EnableNextKey}" Key="Right" />
        </i:Interaction.Behaviors>
        <Carousel.Styles>
          <Style Selector="TextBlock.text">
            <Setter Property="DockPanel.Dock" Value="Bottom" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="Opacity" Value="0.5" />
            <Setter Property="LineHeight" Value="22" />
            <Setter Property="FontWeight" Value="Light" />
          </Style>

          <Style Selector="TextBlock.title">
            <Setter Property="DockPanel.Dock" Value="Bottom" />
            <Setter Property="TextWrapping" Value="Wrap" />
            <Setter Property="TextAlignment" Value="Left" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="FontSize" Value="31" />
            <Setter Property="Margin" Value="0 10" />
          </Style>

          <Style Selector="Viewbox.icon">
            <Setter Property="MaxHeight" Value="50" />
            <Setter Property="DockPanel.Dock" Value="Bottom" />
            <Setter Property="HorizontalAlignment" Value="Left" />
          </Style>
        </Carousel.Styles>
        <Carousel.PageTransition>
          <PageSlide Duration="0:0:0.5" SlideOutEasing="{StaticResource FluentEasing}"
                     SlideInEasing="{StaticResource FluentEasing}" />
        </Carousel.PageTransition>

        <!-- Welcome to Wasabi -->
        <Grid ColumnDefinitions="Auto,*">
          <DockPanel Grid.Column="1" VerticalAlignment="Center">
            <TextBlock Classes="text"
                       Text="Satoshi brought scarcity, while Wasabi is bringing fungibility. As inflation is running rampant, vigilant individuals are opting out.&#10;Bitcoin is rapidly becoming the world's reserve currency, but its fungibility - a property of good money - is still unsatisfactory.&#10;&#10;Wasabi Wallet is a bastion of hope in the quest to qualitatively improve the money of the future." />
            <TextBlock Text="Welcome to Wasabi"
                       Classes="title" />
          </DockPanel>
          <Panel MaxWidth="230" Grid.Column="0"
                 VerticalAlignment="Center"
                 HorizontalAlignment="Right"
                 Margin="0,0,30,0">
            <ExperimentalAcrylicBorder Margin="2">
              <ExperimentalAcrylicBorder.Material>
                <ExperimentalAcrylicMaterial BackgroundSource="Digger"
                                             TintColor="White"
                                             TintOpacity="0"
                                             FallbackColor="White"
                                             MaterialOpacity="0" />
              </ExperimentalAcrylicBorder.Material>
            </ExperimentalAcrylicBorder>
            <Viewbox>
              <Panel>
                <Border Width="465.5" Height="465.5" />
                <Path Fill="{DynamicResource TextControlForeground}" Opacity="0.75"
                      Data="M 14 37 L 14 452 L 453 452 L 453 189 L 322 189 L 322 37 L 14 37 z " />
                <Path Fill="#FF77C200" Opacity="0.75" Data="M 322 18 H 458 V 189 H 322 Z" />
                <Path Fill="{DynamicResource RegionColor}"
                      Data="M0 0v465.5h465.5V0H0zm363.6 76.1h74.6v73.3h-74.6V76zM91.1 81.6a479.5 479.5 0 0 0 54.2 142.3l6.8 9.5h84a474.6 474.6 0 0 1-43.3-115.2L255 82.9a528 528 0 0 0 55.6 141l6.8 9.5h119.3v58.3h-65c19 20.4 40.6 35.3 59.6 47.5l-51.5 50.2c-25.8-17.7-70.5-50.2-107.1-97.7H209c20.4 17.7 44.8 39.4 58.3 47.5l-54.2 50.2A430 430 0 0 1 96.5 274.1a481 481 0 0 1-69.2-156c23-13.5 40.7-21.6 63.8-36.5z" />
              </Panel>
            </Viewbox>
          </Panel>
        </Grid>
        <!-- Non-Custodial -->
        <Grid ColumnDefinitions="*,*">
          <Viewbox Classes="icon" HorizontalAlignment="Left" VerticalAlignment="Top" Height="48" Width="48">
            <ContentControl Template="{DynamicResource wasabi_logo_dynamic}" />
          </Viewbox>
          <DockPanel Grid.Column="0" Grid.Row="0" VerticalAlignment="Center">
            <TextBlock Classes="text"
                       Text="If you don't control your private keys, then you are not the true owner of your funds.&#10;&#10;Wasabi is non-custodial: the developers cannot steal or freeze your funds." />
            <TextBlock Text="Non-Custodial"
                       Classes="title" />
          </DockPanel>
          <Panel Grid.Column="1" MaxHeight="300" Margin="30 0 0 0">
            <Border CornerRadius="4"
                    BoxShadow="0 5 16 0 #3F000000"
                    VerticalAlignment="Top" HorizontalAlignment="Left"
                    Margin="20">
              <Panel>
                <ExperimentalAcrylicBorder CornerRadius="4">
                  <ExperimentalAcrylicBorder.Material>
                    <ExperimentalAcrylicMaterial BackgroundSource="Digger"
                                                 TintColor="{DynamicResource TileRegionColor}"
                                                 TintOpacity="0"
                                                 FallbackColor="{DynamicResource TileRegionColor}"
                                                 MaterialOpacity="0" />
                  </ExperimentalAcrylicBorder.Material>
                </ExperimentalAcrylicBorder>
                <Border Opacity="0.95" CornerRadius="4" Background="{DynamicResource TileRegionColor}" />
                <Viewbox MaxWidth="200" MaxHeight="300">
                  <Border Width="375" Height="263">
                    <Path Fill="{DynamicResource TextControlForeground}"
                          Data="M49.11 35.8v17.45h2.12v-6.82h4.26l3.68 6.82h2.45l-3.94-7.21c2.22-.76 3.26-2.57 3.26-4.9 0-3.08-1.84-5.35-5.93-5.35h-5.9zm111.14 0 4.77 17.45h2.19l3.85-13.9h.13l3.85 13.9h2.19L182 35.79h-2.15l-3.64 14.22h-.18l-3.71-14.22h-2.39l-3.71 14.22h-.18L162.4 35.8h-2.15zm54.65 0v6.44h-.17c-.45-.68-1.23-2.25-3.79-2.25-3.3 0-5.59 2.62-5.59 6.75 0 4.16 2.29 6.78 5.56 6.78 2.52 0 3.37-1.57 3.82-2.28h.24v2.01h1.94V35.79h-2.01zM30.97 37a.54.54 0 0 0-.4.15l-6.85 6.6c-.3.28-.3.76 0 1.04l6.86 6.6a.55.55 0 0 0 .75-.79l-6.01-5.78h13.64a.54.54 0 1 0 0-1.1H25.32l6.01-5.78a.54.54 0 0 0-.36-.94zm20.26.67h3.71c2.83 0 3.92 1.38 3.92 3.48s-1.09 3.37-3.88 3.37h-3.75v-6.85zm86.38 2.29c-1.56 0-2.93.88-3.4 2.18h-.14v-1.98h-1.94v13.09h2v-8.28c0-1.78 1.4-3.07 3.31-3.07.54 0 1.1.1 1.23.13V40c-.23-.02-.76-.03-1.06-.03zm65.32 0c-1.57 0-2.93.88-3.4 2.18h-.14v-1.98h-1.95v13.09h2.02v-8.28c0-1.78 1.4-3.07 3.3-3.07.54 0 1.1.1 1.23.13V40c-.23-.02-.76-.03-1.06-.03zm-133.6.03c-3.55 0-5.9 2.8-5.9 6.82 0 4.02 2.32 6.71 6.1 6.71 2.66 0 4.6-1.33 5.22-3.3l-1.94-.55c-.51 1.36-1.7 2.05-3.28 2.05-2.36 0-3.99-1.53-4.08-4.33h9.5v-.86c0-4.87-2.9-6.54-5.62-6.54zm13.98 0c-3.55 0-5.9 2.8-5.9 6.78 0 3.92 2.25 6.75 5.93 6.75 2.9 0 4.8-1.77 5.15-4.09h-2.01c-.38 1.43-1.57 2.29-3.14 2.29-2.38 0-3.92-1.98-3.92-5.01 0-2.97 1.57-4.92 3.92-4.92 1.77 0 2.83 1.1 3.14 2.29h2c-.33-2.45-2.41-4.1-5.17-4.1zm13.43 0c-3.55 0-5.93 2.7-5.93 6.78 0 4.06 2.38 6.75 5.93 6.75 3.54 0 5.93-2.69 5.93-6.75 0-4.09-2.39-6.78-5.93-6.78zm26.7 0c-3.55 0-5.9 2.8-5.9 6.82 0 4.02 2.31 6.71 6.1 6.71 2.66 0 4.6-1.33 5.21-3.3l-1.94-.55c-.51 1.36-1.7 2.05-3.27 2.05-2.36 0-4-1.53-4.09-4.33h9.5v-.86c0-4.87-2.9-6.54-5.62-6.54zm65 0c-3.54 0-5.93 2.7-5.93 6.78 0 4.06 2.39 6.75 5.93 6.75 3.55 0 5.94-2.69 5.94-6.75 0-4.09-2.4-6.78-5.94-6.78zm37.06 0c-2.76 0-4.8 1.57-4.8 3.78 0 1.81 1.07 3.02 3.47 3.58l2.18.51c1.32.31 1.94.94 1.94 1.85 0 1.12-1.19 2.04-3.06 2.04-1.65 0-2.68-.7-3.04-2.11l-1.9.47c.46 2.23 2.3 3.41 4.97 3.41 3.04 0 5.12-1.66 5