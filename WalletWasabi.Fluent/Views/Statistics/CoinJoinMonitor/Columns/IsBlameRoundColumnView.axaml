<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:cjm="clr-namespace:WalletWasabi.Fluent.ViewModels.Statistics.CoinJoinMonitor"
             xmlns:c="using:WalletWasabi.Fluent.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="cjm:RoundStateViewModel"
             x:CompileBindings="True"
             x:Class="WalletWasabi.Fluent.Views.Statistics.CoinJoinMonitor.Columns.IsBlameRoundColumnView">
  <c:PrivacyContentControl Content="{Binding IsBlameRound}"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="0 0 0 0"
                           Classes="monoSpaced" />
</UserControl>
