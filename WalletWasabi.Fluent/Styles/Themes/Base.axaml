<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Styles.Resources>
    <Color x:Key="SystemAccentColorLight1">#FF82C690</Color>
    <Color x:Key="SystemAccentColorLight2">#FF92CF9E</Color>
    <Color x:Key="SystemAccentColorLight3">#FFA2D8AD</Color>
    <Color x:Key="SystemAccentColorDark1">#FF5BA96C</Color>
    <Color x:Key="SystemAccentColorDark2">#FF449556</Color>
    <Color x:Key="SystemAccentColorDark3">#FF2E8041</Color>
    <Color x:Key="SystemAccentColor">#FF72BD81</Color>
    <Color x:Key="SystemErrorTextColor">#D85050</Color>
    <SolidColorBrush x:Key="TextControlBorderBrushFocused" Color="{StaticResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="ActionButtonForegroundColor">#FFFFFF</SolidColorBrush>
    <SolidColorBrush x:Key="ActionButtonBackgroundColor" Color="{StaticResource SystemAccentColor}"/>
    <SolidColorBrush x:Key="ActionButtonPressedBackgroundColor" Color="#59B164"/>
    <SolidColorBrush x:Key="ActionButtonPointerOverBackgroundColor" Color="#90CB9C"/>
    <SolidColorBrush x:Key="ActionButtonDisabledBackgroundColor" Color="#80A3A3A3"/>

    <SolidColorBrush x:Key="PrivacyLevelNoneBrush" Color="#C95050" />
    <SolidColorBrush x:Key="PrivacyLevelMinimalBrush" Color="#E06868" />
    <SolidColorBrush x:Key="PrivacyLevelMediumBrush" Color="#DDA73E" />
    <SolidColorBrush x:Key="PrivacyLevelStrongBrush" Color="#72BD81" />
    <SolidColorBrush x:Key="PrivacyLevelExtremeBrush" Color="#89C67A" />
    <SolidColorBrush x:Key="PrivacyLevelExtremeAltBrush" Color="#5B92D2" />

    <SolidColorBrush x:Key="WarningMessageForeground">#DDA73E</SolidColorBrush>
    <StaticResource x:Key="SystemControlErrorTextForegroundBrush" ResourceKey="SystemErrorTextColor" />
    <SolidColorBrush x:Key="HyperLinkForeground">#00A2E8</SolidColorBrush>

    <SolidColorBrush x:Key="DataGridRowIndicatorBrush" Color="{StaticResource SystemAccentColor}" />
    <SolidColorBrush x:Key="DataGridColumnHeaderBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridCellFocusVisualPrimaryBrush" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridCellFocusVisualSecondaryBrush" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridRowSelectedHoveredBackgroundColor" Color="Transparent" />
    <SolidColorBrush x:Key="DataGridRowSelectedHoveredUnfocusedBackgroundColor" Color="Transparent" />
    <x:Double x:Key="DataGridRowSelectedBackgroundOpacity">1.0</x:Double>
    <x:Double x:Key="DataGridRowSelectedUnfocusedBackgroundOpacity">1.0</x:Double>

    <x:String x:Key="LabelsWatermarkText">i.e. Alice, Bob, Wasabi Inc, Exchange</x:String>

    <StaticResource x:Key="NotificationCardInformationBackgroundBrush" ResourceKey="SystemAccentColor" />
  </Styles.Resources>
</Styles>
